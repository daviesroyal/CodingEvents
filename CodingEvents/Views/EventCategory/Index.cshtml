@using CodingEvents.Models;
@model List<EventCategory>

<h1>Event Categories</h1>

@if (Model.Count == 0)
{
    <p>No categories yet.</p>
}
else
{
    <table class="table">
        <tr>
            <th>Name</th>
        </tr>
        @foreach (var cat in Model)
        {
            <tr>
                <td>@cat.Name</td>
            </tr>
        }
    </table>
}